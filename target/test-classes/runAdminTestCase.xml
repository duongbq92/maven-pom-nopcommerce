<?xml version="1.0" encoding="UTF-8"?>
<suite name="Nop Commerce Admin" parallel="false">
<listeners>
	<listener class-name = "commons.MethodListener"></listener>
	<listener class-name = "org.uncommons.reportng.HTMLReporter"></listener>
	<listener class-name = "org.uncommons.reportng.JUnitXMLReporter"></listener>
</listeners>
<test name="Run on CHROME UI">
	<parameter name="browser" value="chrome_ui" />
	<parameter name="url" value="https://admin-demo.nopcommerce.com/" />
	<classes>
		<class	name="com.nopcommerce.admin.Level_09_Web_Data_Table_And_Upload" />
	</classes>
</test> 
<test name="Run on FIREFOX UI">
	<parameter name="browser" value="firefox_ui" />
	<parameter name="url" value="https://admin-demo.nopcommerce.com/" />
	<classes>
		<class	name="com.nopcommerce.admin.Level_09_Web_Data_Table_And_Upload" />
	</classes>
</test> 
</suite> <!-- Suite -->
